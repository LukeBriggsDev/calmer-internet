{
    "manifest_version": 2,
    "name": "Calmer Internet",
    "version": "1.0.0",
    "icons": {
      "48": "icons/48.png",
      "96": "icons/96.png"
    },
    "description": "Provides a calmer internet experience",
    "content_scripts": [
      {
        "matches": [
          "*://*.youtube.com/*", 
          "*://*.twitter.com/*", "*://twitter.com/*", 
          "*://*.instagram.com/*"
        ],
        "js": ["calmer.js"]
      }
    ]
  }